[sub_resource type="GDScript" id=4]
script/source = "extends VBoxContainer
#Shop controler
const sp_item = preload(\"res://game/gui/Shop_item.tscn\")



func _ready():
	var shop_item
	for unit in owner.owner.get_node(\"units\").get_children():
		shop_item = sp_item.instance()
		shop_item.get_node(\"HBoxContainer/VBoxContainer/name\").text= unit.name
		shop_item.get_node(\"HBoxContainer/VBoxContainer/rumpf\").texture =unit.get_node(\"KinematicBody2D/rumpf\").texture
		shop_item.get_node(\"HBoxContainer/VBoxContainer/rumpf/gun\").texture=unit.get_node(\"KinematicBody2D/KinematicBody_Waffe/Waffe\").texture
		shop_item.Shop_ID = unit #über gist die Objekt id anden buton
		add_child(shop_item)
"

